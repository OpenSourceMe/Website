{
  "name": "OpenSourceMe-Website",
  "version": "0.0.1",
  "description": "A server-side website engine for your OpenSourceMe repo",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "browser:dev": "NODE_ENV='development' webpack-dev-server --progress",
    "server": "NODE_ENV='production' PORT=3000 node entry.js",
    "server:dev": "npm run build && nodemon --exec NODE_ENV='production' PORT=3000 node entry.js",
    "test": "babel-tape-runner src/**/*.spec.js | tap-spec",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint --config=.eslintrc src/** src/**/*.js !src/api/data/**/*.js",
    "start": "npm run build && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenSourceMe/Website"
  },
  "keywords": [
    "OpenSourceMe",
    "Website",
    "React",
    "Redux",
    "Webpack",
    "Babel"
  ],
  "author": "Lachlan Kermode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenSourceMe/Website/issues"
  },
  "homepage": "https://github.com/OpenSourceMe/Website#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "bootstrap": "^3.3.6",
    "express": "^4.13.4",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash-node": "latest",
    "markdown": "^0.5.0",
    "node-libs-browser": "0.5.3",
    "radium": "^0.16.6",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.0.4",
    "react-remarkable": "^1.1.1",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.5",
    "redux-multi": "^0.1.12",
    "redux-thunk": "^1.0.3",
    "request": "^2.72.0",
    "request-promise": "^3.0.0",
    "serialize-javascript": "^1.1.2"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "^2.0.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^3.16.1",
    "file-loader": "0.8.4",
    "mockery": "^1.6.2",
    "nock": "^3.6.0",
    "nodemon": "^1.9.1",
    "raw-loader": "^0.5.1",
    "redux-mock-store": "0.0.4",
    "rewire": "^2.5.1",
    "sinon": "^1.17.3",
    "style-loader": "0.13.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "underscore": "^1.8.3",
    "url-loader": "0.5.6",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
