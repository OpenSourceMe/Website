{
  "name": "danboiler-react-redux",
  "version": "0.0.1",
  "description": "Boiler plate for react-redux application",
  "main": "''",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "dev": "NODE_ENV='development' webpack-dev-server --progress --quiet",
    "server": "NODE_ENV='production' PORT=3000 node entry.js",
    "server:dev": "nodemon --exec PORT=3000 node entry.js",
    "test": "babel-tape-runner test/**/*.test.js | tap-spec",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src",
    "start": "npm run build && npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielkermode/danboiler-react-redux"
  },
  "keywords": [
